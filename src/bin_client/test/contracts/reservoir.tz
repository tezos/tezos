parameter unit ;
storage
  (pair
     (pair (timestamp @T) (tez @N))
     (pair (contract @A unit) (contract @B  unit))) ;
code
  { CDR ; DUP ; CAAR ; # T
    NOW ; COMPARE ; LE ;
    IF { DUP ; CADR ; # N
         BALANCE ;
         COMPARE ; LE ;
         IF { NIL operation ; PAIR }
            { DUP ; CDDR ; # B
              BALANCE ; UNIT ;
              TRANSFER_TOKENS ;
              NIL operation ; SWAP ; CONS ;
              PAIR } }
       { DUP ; CDAR ; # A
         BALANCE ;
         UNIT ;
         TRANSFER_TOKENS ;
         NIL operation ; SWAP ; CONS ;
         PAIR } }
